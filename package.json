{
  "name": "aptos_token",
  "description": "Aptos Token SDK",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=11.0.0"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "rimraf dist types && cross-env NODE_ENV=development webpack --config ./scripts/webpack.dev.js",
    "build": "rimraf dist types && cross-env NODE_ENV=production webpack --config ./scripts/webpack.prod.js",
    "test": "jest",
    "clean": "rm -rf node_modules && npm install",
    "typedoc": "rimraf docs && typedoc",
    "fmt": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://git@github.com:aptos-labs/token.git"
  },
  "author": "aptoslabs.com",
  "keywords": [
    "Aptos",
    "Aptos Labs",
    "Move"
  ],
  "dependencies": {
    "@bundlr-network/client": "^0.9.0-a6",
    "@noble/hashes": "1.1.2",
    "@scure/bip39": "1.1.0",
    "aptos": "1.3.15",
    "axios": "0.27.2",
    "big-number": "^2.0.0",
    "crypto": "^1.0.1",
    "form-data": "4.0.0",
    "tweetnacl": "1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "@types/jest": "28.1.8",
    "@types/node": "18.6.2",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "dotenv": "16.0.2",
    "eslint": "8.23.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "^28.1.3",
    "openapi-typescript-codegen": "https://github.com/aptos-labs/openapi-typescript-codegen/releases/download/v0.23.0-p5/openapi-typescript-codegen-v0.23.0-p5.tgz",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "10.9.1",
    "tsup": "6.2.3",
    "typedoc": "^0.23.16",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpackbar": "^5.0.2"
  },
  "version": "0.0.1"
}
